package br.com.fiap.joao.persistencia01.JDBCTemplate.entity;

public class Nota {
	
	private Aluno aluno;
	private Curso curso;
	private Double nota;
	
	public Double getNota() {
		return nota;
	}
	public void setNota(Double nota) {
		this.nota = nota;
	}
	public Aluno getAluno() {
		return aluno;
	}
	public void setAluno(int matricula) {
		this.aluno.setMatricula(matricula);
	}
	public Curso getCurso() {
		return curso;
	}
	public void setCurso(int curso) {
		this.curso.setIdCurso(curso);
	}

}
